{
  "name": "mehr-als-nix/notifier",
  "description": "PHPUnit notify listener",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Siad Ardroumli",
      "email": "siad.ardroumli@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "nels-o/toaster",
        "version": "dev-master",
        "dist": {
          "type": "zip",
          "url": "https://github.com/nels-o/toaster/archive/master.zip"
        }
      }
    }
